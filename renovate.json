{
    "groupName": "all dependencies",
    "separateMajorMinor": false,
    "groupSlug": "all",
    "hostRules": [
        {
          "hostType": "maven",
          "matchHost": "https://maven.pkg.github.com/dishantkamble/central-java-dependency/",
          "token": "${{ github.token }}"
        }
    ],
    "packageRules": [
      {
        "matchPackagePatterns": [
          "*"
        ],
        "groupName": "all dependencies",
        "groupSlug": "all"
      },
      {
        "matchDatasources": ["maven"],
        "registryUrls": ["https://maven.pkg.github.com/dishantkamble/central-java-dependency/", "https://repo.maven.apache.org/maven2"]
      }
    ],
    "lockFileMaintenance": {
      "enabled": false
    }
}
